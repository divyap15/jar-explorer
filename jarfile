pipeline {
     agent any
     tools {
      maven "maven"
      }
      stages {
      stage("BUILD stage") {
	     steps {
	          sh 'mvn clean package'
	     }
      }
       stage ("RUN stage") {
            step {
    stage('archive') {
    steps {
      archiveArtifacts artifacts: 'target/*.jar', followSymlinks: false
    }
  }
  
   }
}
